<p id="notice"><%= notice %></p>

<h1>Listing Remote Projects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @remote_projects.each do |remote_project| %>
      <tr>
        <td><%= link_to 'Show', remote_project %></td>
        <td><%= link_to 'Edit', edit_remote_project_path(remote_project) %></td>
        <td><%= link_to 'Destroy', remote_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Remote project', new_remote_project_path %>
