<h1>Which Project to Clone?</h1>
<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @remote_projects.entities.each do |remote_project| %>
    <tr>
      <td>
        <%= link_to remote_project.name, new_project_path({source_remote_id: remote_project.id}) %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>